{% extends 'base.html.twig' %}

{% block title %}
	{{parent()}}
	- Ajout Utilisateur
{% endblock %}

{% block body %}
	{{parent()}}
	<div class="container">
		<div class="row justify-content-center">
			<h1 class="text-center text-primary p-4">Ajout d'un fichier
			</h1>
		</div>
		<div class="row justify-content-center">
			<div class="col-12 bg-white p-4 m-0 text-primary">
				{{ form_start(form) }}
				<div class="form-group">
               
					{{ form_label(form.nom, 'Fichier ', {'label_attr': {'class': 'font-weight-bold'}}) }}
					{{ form_widget(form.nom, {'attr': {'class': 'form-control'}}) }}
                    	
				</div>
                {{form_end(form) }}
			</div>
		</div>
	</div>




{% endblock %}
